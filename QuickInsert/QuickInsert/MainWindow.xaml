<Window x:Name="form" x:Class="QuickInsert.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuickInsert"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1024" Loaded="Window_Loaded" SizeChanged="Window_SizeChanged">
    <StackPanel x:Name="stackPanel" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

        <ToolBarPanel x:Name="toolBarPanel" >
            <ToolBar >
                <Button Content="Save" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Button_Click"/>
                <Button Content="Form" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Form_Button_Click"/>
            </ToolBar>
        </ToolBarPanel>
        <TextBox 
            x:Name="textBoxItems"
            TextWrapping="NoWrap" 
            HorizontalAlignment="Left" 
            Background="#FFDDD5F4" AcceptsTab="True" AcceptsReturn="True" Width="873" FontWeight="Regular" FontFamily="Candara" FontSize="14"/>

    </StackPanel>
</Window>
